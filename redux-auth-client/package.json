    "storybook-addon-specifications": "^1.0.16",
    "wallaby-webpack": "0.0.34",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^4.0.0"
  },
  "dependencies": {
