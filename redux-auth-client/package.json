{
  "name": "redux-starter",
  "version": "1.0.0",
  "description": "Redux starter package with WallabyJs, Karma, React Storybook, and Flow",
  "main": "index.js",
  "repository": "git@github.com:gregorskii/redux-starter.git",
  "scripts": {
    "dev": "NODE_ENV=dev node build/dev-server.js",
    "storybook": "NODE_ENV=storybook start-storybook -p 9001",
    "test": "NODE_ENV=test mocha './src/**/*-spec.*' --compilers js:babel-core/register --require=./test/unit/spec/helpers/index.js --recursive",
    "lint": "eslint --ext .js,.jsx src test/unit/spec",
    "build": "rm -rf ./dist && export NODE_ENV=prod && webpack --config ./build/webpack.prod.conf.js -p"
  },
  "author": "gregorskii",
  "license": "MIT",
  "browserslist": [
    "last 1 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@kadira/storybook": "v2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "compression-webpack-plugin": "^0.3.2",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.2.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "jsdom": "9.12.0",
    "jsdom-global": "2.1.1",
    "json-loader": "^0.5.4",
